<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>9.0</LangVersion>
	</PropertyGroup>

	<PropertyGroup>
		<Version>1.0.0.1</Version>
		<PackageReleaseNotes>Запуск проекта</PackageReleaseNotes>
	</PropertyGroup>

	<PropertyGroup>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>MetaWeatherService.pfx</AssemblyOriginatorKeyFile>
		<Authors>vasilyeu.uv@mail.ru</Authors>
		<Description>Клиент для MetaWeatherApi https://www.metaweather.com/api/</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/VasilyeuUV/MetaWeather</PackageProjectUrl>
		<RepositoryUrl>https://github.com/VasilyeuUV/MetaWeather.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>MetaWeatherService, MetaWeather, Weather, MetaWeatherClient, WebAPI</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
		<PackageReference Include="System.Net.Http.Json" Version="6.0.0" />
	</ItemGroup>

</Project>
